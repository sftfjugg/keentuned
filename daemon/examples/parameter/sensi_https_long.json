{
    "sysctl":{
        "net.core.optmem_max":{
            "dtype":"int",
            "range":[20480,204800],
            "step":20480
        },
        "vm.watermark_scale_factor":{
            "dtype":"int",
            "range":[10,1000],
            "step":10
        },
        "net.ipv4.conf.all.secure_redirects":{
            "dtype":"string",
            "options":["0","1"]
        },
        "net.core.dev_weight":{
            "dtype":"int",
            "range":[16,1024]
        },
        "net.ipv4.tcp_ecn":{
            "dtype":"string",
            "options":["0","1","2"]
        },
        "net.ipv4.tcp_sack":{
            "dtype":"string",
            "options":["0","1"]
        }
    },
    "nginx":{
        "ssl_buffer_size": {
            "dtype": "string",
            "options": ["64","128","526","4k","64k","256k","1m","4m"]
        },
        "keepalive_requests": {
            "dtype": "int",
            "range": [10,1000]
        },
        "client_header_buffer_size": {
            "dtype": "string",
            "options": ["2k","4k","8k","16k","32k","64k"]
        },
        "sendfile": {
            "dtype": "string",
            "options": ["on","off"]
        }
    }
}